{
  "name": "vanessador",
  "version": "0.0.1",
  "description": "a firebase app with typeform requests",
  "main": "bin/www",
  "scripts": {
    "build:API:firebase": "grunt build:firebase:apiKey ; grunt build:firebase:messagingSenderId",
    "build:API:typeform": "grunt build:typeform:apiKey",
    "build:API:pagseguro": "grunt build:pagseguro:apiKey ; grunt build:pagseguro:email",
    "build:API:mailgun": "grunt build:mailgun:apiKey ; grunt build:mailgun:domain",
    "build:API": "npm run build:API:firebase ; npm run build:API:typeform ; npm run build:API:pagseguro ; npm run build:API:mailgun",
    "build:docs": "grunt build:docs shell:docco",
    "build:app": "grunt build:init build:libs coffee usebanner --verbose",
    "test:server": "mocha --timeout 15000 bin/test.js",
    "test:client": "testcafe remote bin/test/ --quarantine --ports 1337,1338 --proxy-host https://iqzmbsjy.p19.weaved.com/ --qr-code",
    "test": "npm run test:server ; test:client",
    "serve": "./bin/www &",
    "start": "killall node ; npm run build:app && npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "https://www.github.com/lunhg/vanessador.git"
  },
  "author": [
    "Guilherme Lunhani <lunhg@gmail.com>"
  ],
  "license": "SEE LICENSE IN LICENSE.md",
  "dependencies": {
    "atob": "^2.0.3",
    "body-parser": "^1.17.2",
    "chalk": "^2.0.1",
    "compression": "^1.7.0",
    "connect-assets": "^5.4.0",
    "express": "^4.15.3",
    "foreach": "^2.0.5",
    "jstransformer-marked": "^1.0.2",
    "keytar": "^4.0.3",
    "morgan": "^1.8.2",
    "node-schedule": "^1.2.4",
    "node-uuid": "^1.4.8",
    "nodemailer": "^4.1.0",
    "nodemailer-mailgun-transport": "^1.3.5",
    "paypal-rest-sdk": "^1.7.1",
    "pug": "^2.0.0-rc.2",
    "request": "^2.81.0",
    "request-json": "^0.6.2",
    "request-promise": "^4.2.1",
    "testcafe": "^0.17.2",
    "testcafe-vue-selectors": "^2.0.0",
    "xml2js": "^0.4.17",
    "xmlbuilder": "^9.0.1"
  },
  "devDependencies": {
    "chalk": "^2.0.1",
    "check_node": "^1.0.0",
    "grunt": "^1.0.1",
    "grunt-banner": "^0.6.0",
    "grunt-contrib-coffee": "^1.0.0",
    "grunt-shell": "^2.1.0",
    "mocha": "^3.4.2",
    "mocha-testdata": "^1.2.0",
    "require-from-package": "0.0.4",
    "should": "^11.2.1",
    "supertest": "^3.0.0",
    "syncprompt": "^1.0.3",
    "uuid": "^3.1.0"
  },
  "firebase": {
    "project": {
      "name": "vanessador-46e68"
    }
  },
  "options": {
    "coffee": {
      "compileJoin": {
        "options": {
          "join": true,
          "bare": true
        },
        "files": {
          "bin/www": [
            "boot/dependencies.coffee",
            "config/environment.coffee",
            "config/app.coffee",
            "config/pagseguro.coffee",
            "config/server.coffee",
            "boot/app.coffee",
            "app/controllers/index.coffee",
            "app/controllers/config.coffee",
            "app/controllers/templates.coffee",
            "app/controllers/services.coffee",
            "app/controllers/typeform.coffee",
            "app/controllers/pagseguro.coffee",
            "app/controllers/mailer.coffee",
            "app/controllers/docs.coffee",
            "boot/server.coffee"
          ],
          "bin/test.js": [
            "boot/devDependencies.coffee",
            "test/agent.coffee",
            "test/pagseguro.coffee"
          ],
          "bin/test/login.test.js": [
            "test/browser_init.coffee",
            "test/login.coffee"
          ]
        }
      }
    },
    "usebanner": {
      "www": {
        "options": {
          "position": "top",
          "banner": "#!/usr/bin/env <%= pkg.node_version %>",
          "linebreak": true
        },
        "files": {
          "src": "bin/www"
        }
      }
    }
  }
}
