section
  br
  br
  br
  div.container
    h1 Boletos
    accordion(type="success")
      panel(header="Importart Excel") 
        form
          fieldset
            div.form-group(v-for="(v,k) in modelos.xls.boletos")
              label.control-label(:for="k") {{ v.label }}
              br
              div
                input.form-control(:type="v.type" :id="k" :placeholder="v.placeholder")
        button.btn-btn-default(type="file" :name='boletos' v-on:change='importarXLS')

  div.container  
    accordion(type="info" :one-at-time='true')  
      panel(v-for="(v,k) in boletos" :data="v" :key="k" :header="k")
        strong(v-for="(val,key) in v")
          u(:id="key") {{ key }}: {{ val }}
          <router-link :to="{path: '/boletos/'+k+'/editar?q='+key}"> Atualizar</router-link>
          br
